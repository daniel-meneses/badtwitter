import React, { useState } from "react";

export default function SubscriptionList(props: any) {

  const doThing = (e :any) => {
    let a = e.currentTarget.parentNode.getAttribute("data-key");
  }

  return (
    { this.props.request_list.length ?
      this.props.request_list.map((r :any) =>
      <SubscriptionRequest key={r.id}
                            subscription={r}
                            denyRequest={this.updateRequest}
                            acceptRequest={this.updateRequest}/>)
      :
      <span>You have no Subscription Requests</span>
    }
  );
}
