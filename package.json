    "wait-on": "^5.2.0"
  },
  "scripts": {
    "dev:build:client": "webpack -wd --mode development --config ./webpack/webpack.client.config.js",
    "dev:build:server": "webpack -wd --mode development --config ./webpack/webpack.server.config.js",
    "dev:build": "npm-run-all -p dev:build:client dev:build:server",
    "dev:run": "nodemon -d dist/app.server.js",
    "cy:build:client": "webpack --mode development --config ./webpack/webpack.client.config.js",
    "cy:build:server": "webpack --mode development --config ./webpack/webpack.server.config.js",
    "cy:build": "ENV=CY_TEST npm-run-all -p cy:build:client cy:build:server",
    "cy:run": "ENV=CY_TEST npm-run-all -s cy:build start",
    "cy:test": "ENV=CY_TEST cypress run --headed --no-exit",
    "test": "npm-run-all -s cy:run cy:test",
    "dev": "nodemon dist/app.server.js",
    "prod:build:client": "webpack --mode production --config ./webpack/webpack.client.config.js",
    "prod:build:server": "webpack --mode production --config ./webpack/webpack.server.config.js",
    "prod:build": "npm-run-all -p prod:build:client prod:build:server",
    "start": "node --max_old_space_size=2560 dist/app.server.js & wait-on http://localhost:3000",
    "heroku-postbuild": "webpack -p --config ./webpack/webpack.client.config.js && webpack -p --config ./webpack/webpack.server.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
